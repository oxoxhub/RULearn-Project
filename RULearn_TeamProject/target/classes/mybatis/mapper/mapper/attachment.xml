<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="attachImageMapper">
	<select id="getAttachList" resultType="AttachImageDto">
		SELECT *
		  FROM vam_image
		 WHERE L_ID = #{L_ID}
	</select>
</mapper>