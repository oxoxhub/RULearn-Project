<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="userInfoMapper">
	<select id="selectUserData" resultType="accountDto">
		SELECT AC_ID
			 , AC_NICKNAME
		  FROM ACCOUNT
		 WHERE AC_ID = #{id}
	</select>
	
</mapper>